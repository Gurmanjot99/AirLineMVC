
@{
    ViewBag.Title = "MainLogin";
}

<div class="container">
    <div class="row">
        
        <div class="col-md-12">

            <form method="POST" action="LoginQuery">
                
                <label>Enter Email </label> <br />
                <input type="email" name="txtName" placeholder="Enter Email" class="form-control" required /><br /><br />

                <label>Enter Password </label> <br />
                <input type="password" name="txtPassword" placeholder="Enter Password" class="form-control" required /><br /><br />

                <input type="submit" value="Submit" class="btn btn-primary" />

            </form>
        </div>
    </div>

</div>